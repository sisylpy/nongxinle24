            <if test="nxDwOrderNames != null">`nx_dw_order_names` = #{nxDwOrderNames}</if>
        </set>
        where nx_distributer_weight_id = #{nxDistributerWeightId}
    </update>

    <delete id="delete">
        delete
        from nx_distributer_weight
        where nx_distributer_weight_id = #{value}
    </delete>

    <delete id="deleteBatch">
        delete from nx_distributer_weight where nx_distributer_weight_id in
        <foreach item="nxDistributerWeightId" collection="array" open="(" separator="," close=")">
            #{nxDistributerWeightId}
        </foreach>
    </delete>

</mapper>