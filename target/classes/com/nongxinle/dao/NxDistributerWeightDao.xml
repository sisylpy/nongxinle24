        select * from nx_distributer_weight as ndw
        left join nx_distributer_user as ndu on ndu.nx_distributer_user_id = ndw.nx_dw_user_id
         left join nx_department_orders as ndo on ndo.nx_DO_weight_id = ndw.nx_distributer_weight_id
        <where>
            <if test="disId != null">
                and nx_dw_dis_id = #{disId}
            </if>
