            </if>
            <if test="resFatherId != null">
                and nx_dw_res_father_id = #{resFatherId}
            </if>
            <if test="status != null">
                and nx_dw_status &lt; #{status}
            </if>
            <if test="orderStatus != null">
                and nx_DO_status &lt; #{orderStatus}
            </if>
            <if test="equalStatus != null">
                and nx_dw_status =  #{equalStatus}
            </if>
            <if test="type != null">
                and nx_dw_type =  #{type}
            </if>
            <if test="notType != null">
                and nx_dw_type !=  #{notType}
            </if>
            <if test="startDate != null">
                and nx_dw_date &gt;=  #{startDate}
            </if>
            <if test="stopDate != null">
                and nx_dw_date &lt;=  #{stopDate}
            </if>
        </where>
    </select>

